{
	"name": "Azure Portal Search",
	"version": "1.0",
	"omnibox": {
		"keyword": "portal"
	},
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"48": "icons/icon-48.png",
		"64": "icons/icon-64.png"
	},
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"permissions": [
		"cookies",
		"*://*.azure.com/*"
	],
	"description": "Search fast through your Azure Portal resources directly from omnibox",
	"manifest_version": 2
}